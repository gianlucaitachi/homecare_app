# Environment configuration for the HomeCare backend
# Copy this file to `.env` and adjust the values as needed.

# PostgreSQL connection
DB_HOST=localhost
DB_PORT=5432
DB_NAME=homecare
DB_USER=dev
DB_PASS=devpass

# JWT secrets (replace with secure values in production)
JWT_ACCESS_SECRET=change-me-access
JWT_REFRESH_SECRET=change-me-refresh

# API server port
PORT=8080
